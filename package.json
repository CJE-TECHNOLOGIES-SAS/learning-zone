{
  "name": "learning-zone",
  "version": "1.0.0",
  "description": "Learning Zone es una aplicación web educativa diseñada para fortalecer las habilidades en ofimática (Word, PowerPoint y Excel) de los estudiantes de 11° grado de la Institución Educativa Rural Juan Tamayo, en Ciudad Bolívar.",
  "homepage": "https://github.com/CJE-TECHNOLOGIES/learning-zone#readme",
  "bugs": {
    "url": "https://github.com/CJE-TECHNOLOGIES/learning-zone/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CJE-TECHNOLOGIES/learning-zone.git"
  },
  "license": "ISC",
  "author": "CJE Technologies",
  "private": true,
  "workspaces": [
    "frontend",
    "chat-service",
    "backend"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:chat\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:chat": "cd chat-service && npm run dev",
    "dev:backend": "cd backend && python -m uvicorn main:app --reload --host 0.0.0.0 --port 8000",
    "build": "npm run build:frontend && npm run build:chat",
    "build:frontend": "cd frontend && npm run build",
    "build:chat": "cd chat-service && npm run build",
    "start": "concurrently \"npm run start:frontend\" \"npm run start:chat\" \"npm run start:backend\"",
    "start:frontend": "cd frontend && npm run preview",
    "start:chat": "cd chat-service && npm start",
    "start:backend": "cd backend && python -m uvicorn main:app --host 0.0.0.0 --port 8000",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@react-google-maps/api": "^2.20.7",
    "@tsparticles/preset-big-circles": "^3.2.0",
    "@tsparticles/react": "^3.0.0",
    "@types/google.maps": "^3.58.1",
    "camelize": "^1.0.1",
    "css-color-keywords": "^1.0.0",
    "css-to-react-native": "^3.2.0",
    "csstype": "^3.1.3",
    "framer-motion": "^12.23.12",
    "goober": "^2.1.16",
    "lottie-react": "^2.4.1",
    "nanoid": "^3.3.11",
    "picocolors": "^1.1.1",
    "postcss": "^8.4.49",
    "postcss-value-parser": "^4.2.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^5.5.0",
    "scheduler": "^0.26.0",
    "shallowequal": "^1.1.0",
    "source-map-js": "^1.2.1",
    "styled-components": "^6.1.19",
    "stylis": "^4.3.2",
    "tslib": "^2.6.2",
    "tsparticles-engine": "^2.12.0"
  },
  "keywords": []
}
